{
	"name": "caffe-manager-client",
	"version": "0.0.0",
	"description": "Caffe manager client",
	"main": "./dist/electron/main.js",
	"author": "Nikola Jovanovic <nikola.devbusiness@gmail.com> (https://github.com/skinnn)",
	"license": "MIT",
	"scripts": {
		"start": "node .electron-vue/dev-runner.js",
		"dev": "node .electron-vue/dev-runner.js",
		"build": "node .electron-vue/build.js",
		"build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
		"build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
		"build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
		"build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
		"build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
		"build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
		"lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
		"lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
		"pack": "npm run pack:main && npm run pack:renderer",
		"pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
		"pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
		"postinstall": "npm run lint:fix"
	},
	"dependencies": {
		"axios": "^0.18.1",
		"dotenv-webpack": "^1.7.0",
		"jquery": "^3.3.1",
		"material-design-icons": "^3.0.1",
		"sweetalert2": "^7.26.28",
		"uuid": "^3.4.0",
		"vue": "^2.5.18",
		"vue-electron": "^1.0.6",
		"vue-router": "^3.3.2",
		"vuetify": "^1.5.24",
		"vuex": "^3.4.0",
		"vuex-router-sync": "^5.0.0"
	},
	"devDependencies": {
		"babel-core": "^6.25.0",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^7.1.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.24.1",
		"babili-webpack-plugin": "^0.1.2",
		"cfonts": "^1.1.3",
		"chalk": "^2.1.0",
		"copy-webpack-plugin": "^4.0.1",
		"cross-env": "^5.0.5",
		"css-loader": "^0.28.4",
		"del": "^3.0.0",
		"devtron": "^1.4.0",
		"electron": "^9.0.2",
		"electron-debug": "^3.1.0",
		"electron-devtools-installer": "^3.0.0",
		"electron-packager": "^14.2.1",
		"electron-rebuild": "^1.11.0",
		"eslint": "^4.4.1",
		"eslint-config-standard": "^12.0.0-alpha.0",
		"eslint-friendly-formatter": "^3.0.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-html": "^3.1.1",
		"eslint-plugin-import": "^2.7.0",
		"eslint-plugin-node": "^5.1.1",
		"eslint-plugin-promise": "^3.5.0",
		"eslint-plugin-standard": "^3.0.1",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.30.1",
		"multispinner": "^0.2.1",
		"node-loader": "^0.6.0",
		"node-sass": "^4.12.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.18.2",
		"url-loader": "^0.5.9",
		"vue-html-loader": "^1.2.4",
		"vue-loader": "^13.0.5",
		"vue-style-loader": "^3.0.1",
		"vue-template-compiler": "^2.5.18",
		"webpack": "^3.5.2",
		"webpack-dev-server": "^2.7.1",
		"webpack-hot-middleware": "^2.18.2"
	}
}
