{
  "name": "vue-caffe",
  "version": "1.0.0",
  "author": "Nikola Jovanovic <nikola.devbusiness@gmail.com>",
  "description": "App for managing a cafe or a store.",
  "license": "MIT",
  "scripts": {
    "client-install": "cd client && npm install",
    "server-install": "cd server && npm install",
    "install-dependencies": "concurrently \"npm run server-install\" \"npm run client-install\"",
    "client": "npm start --prefix client",
    "server": "npm start --prefix server",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skinnn/vue-caffe.git"
  },
  "author": "Nikola Jovanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skinnn/vue-caffe/issues"
  },
  "homepage": "https://github.com/skinnn/vue-caffe#readme",
  "dependencies": {
    "concurrently": "^3.6.1"
  }
}
